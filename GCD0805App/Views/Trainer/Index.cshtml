@model GCD0805App.Models.IdentityModel
@{
    ViewBag.Title = "Index";
}

<h2>@Model.Name Profile</h2>

@Html.ActionLink("Edit", "Edit", "Trainers", null, new { @class = "btn btn-success" })

<div class="panel panel-info">
    <div class="panel-body">
        <p>Name: @Model.Name</p>
        <p>Age: @Model.Age</p>
        <p>Date of Birth: @Model.DateOfBirth</p>
        <p>Address: @Model.Address</p>
        <p>Specialty: @Model.Specialty</p>
    </div>
</div>